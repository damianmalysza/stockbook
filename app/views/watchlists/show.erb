<div class="page-title"><h1><%=@watchlist.user.username.capitalize%> <em>>></em> <%=@watchlist.name%> Watchlist</h1></div>

<div class="stock-list">
<% @watchlist.stocks.each do |stock| %>
<div class="stock-list-card">
  <% #stock_info = Stock.get_stock_info(stock.ticker)%>
  <div class="stock-list-card-element title"><strong><%=stock.ticker%></strong> - <em><%=stock.company_name%></em></div>
  <div class="stock-list-card-element main">Latest Price: </div>
  <div class="stock-list-card-element side">P/E Ratio: </div>
  <div class="stock-list-card-element main">Change %: </div>
  <div class="stock-list-card-element side">Past Year Highest Price: </div>
  <div class="stock-list-card-element main">Today's Highest Price: </div>
  <div class="stock-list-card-element side">Past Year Lowest Price: </div>
  <div class="stock-list-card-element main">Today's Lowest Price: </div>
  <div class="stock-list-card-element side">Volume: </div>
</div>
<% end %>
</div>

